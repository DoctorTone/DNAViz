: white;\r\n    width: 25%;\r\n    -webkit-touch-callout: none;\r\n}\r\n#rotateControls {\r\n    right: 2%;\r\n    bottom: 15%;\r\n}\r\n#zoomControls {\r\n    right: 2%;\r\n    bottom: 30%;\r\n}\r\n#rotateControls p,\r\n#zoomControls p {\r\n    margin-bottom: 0.5rem;\r\n}\r\n#rotateControls button,\r\n#zoomControls button {\r\n    width: 35%;\r\n    padding: 0;\r\n}\r\n#rotateControls img,\r\n#zoomControls img {\r\n    width: 100%;\r\n}\r\n#message {\r\n    bottom: 5%;\r\n    width: 33.3%;\r\n    left: 33.3%;\r\n    font-size: 1.5vw;\r\n    color: white;\r\n    background-color: #3e3e3e;\r\n    border-radius: 15px;\r\n    padding: 1%;\r\n}\r\n#message img {\r\n    width: 10%;\r\n}\r\n#notSupported {\r\n    top: 20%;\r\n    left: 12.5%;\r\n    width: 75%;\r\n    background-color: #3e3e3e;\r\n    color: white;\r\n    font-size: 1.25vw;\r\n    border-radius: 15px;\r\n    padding: 1%;\r\n}\r\n\r\n/* Very small devices in landscape */\r\n@media (min-width: 576px) and (orientation: landscape) {\r\n    #guiWidth {\r\n        width: 25%;\r\n    }\r\n    #rotateControls,\r\n    #zoomControls {\r\n        width: 17.5%;\r\n    }\r\n    #zoomControls {\r\n        bottom: 37.5%;\r\n    }\r\n}\r\n\r\n@media (min-width: 768px) {\r\n    #guiWidth {\r\n        width: 30%;\r\n    }\r\n    #rotateControls,\r\n    #zoomControls {\r\n        width: 15%;\r\n    }\r\n    #zoomControls {\r\n        bottom: 15%;\r\n    }\r\n    #rotateControls {\r\n        bottom: 5%;\r\n    }\r\n}\r\n\r\n@media (min-width: 992px) {\r\n    #rotateControls,\r\n    #zoomControls {\r\n        width: 12.5%;\r\n    }\r\n    #rotateControls {\r\n        bottom: 3.5%;\r\n    }\r\n}\r\n\r\n@media (min-width: 1200px) {\r\n    #rotateControls,\r\n    #zoomControls {\r\n        width: 9%;\r\n    }\r\n}\r\n\r\n@media (min-width: 1600px) {\r\n    #guiWidth {\r\n        width: 20%;\r\n    }\r\n    #rotateControls,\r\n    #zoomControls {\r\n        width: 7.5%;\r\n    }\r\n}\r\n"},"lineCount":185}},"error":null,"hash":"90